  },
  "keywords": [
    "sloped",
    "angled",
    "edges",
    "sections"
  ],
  "browserslist": [
    "last 2 versions"
  ],
  "scripts": {
    "autoprefix": "postcss example/dist/app.css -u autoprefixer -o example/dist/app.css",
    "sass": "node-sass --output-style expanded --source-map true example/src/app.scss example/dist/app.css",
    "watch": "yarn sass --watch",
